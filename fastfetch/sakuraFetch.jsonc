{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "padding": {
      "top": 0
    },
    "type": "kitty",
    "source": "~/.config/fastfetch/sakurafetch.png",
    "width": 40
  },
  "display": {
    "separator": " -> "
  },
  "modules": [
    "break",
    "break",
    "break",
    "break",
    "break",
    {
      "type": "title",
      "keyWidth": 10,
      "format": " 󰣇    锕乐课褐蓝德 (ARCHLAND)   󰣇 "
    },
    {
      "type": "custom",
      "format": "───────────────────────────────────"
    },
    {
      "type": "kernel",
      "key": " ",
      "keyColor": "yellow"
    },
    {
      "type": "wm",
      "key": " ",
      "keyColor": "blue"
    },
    {
      "type": "shell",
      "key": " ",
      "keyColor": "yellow"
    },
    {
      "type": "terminal",
      "key": " ",
      "keyColor": "blue"
    },
    {
      "type": "packages",
      "key": "󰏖 ",
      "keyColor": "yellow"
    },
    {
      "type": "memory",
      "key": "󰍛 ",
      "keyColor": "magenta",
      "format": "{1} / {2}"
    },
    {
      "type": "uptime",
      "key": "󰔛 ",
      "keyColor": "green"
    },
    {
      "type": "custom",
      "format": "───────────────────────────────────"
    },
    {
      "type": "custom",
      "format": "   \u001b[31m󰴈  \u001b[32m󰴈  \u001b[33m󰴈  \u001b[34m󰴈  \u001b[35m󰴈  \u001b[36m󰴈  \u001b[37m󰴈  \u001b[90m󰴈 "
    },
    "break"
  ]
}
